    "expo-web-browser": "~14.2.0",
    "express": "^5.1.0",
    "firebase": "^12.4.0",
    "haversine-distance": "^1.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-agora": "^4.5.3",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gesture-handler": "~2.24.0",
