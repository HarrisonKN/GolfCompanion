{
  "expo": {
    "name": "Golf Companion",
    "slug": "golf-companion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/golf-logo.png",
    "scheme": "golfcompanion",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.golfcompanion"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/golf-logo.png",
          "imageWidth": 400,
          "resizeMode": "contain",
          "backgroundColor": "#003b23"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
